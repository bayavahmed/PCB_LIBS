		<script type="text/javascript">
		  function openURL(url)
		  {
		      var shell = new ActiveXObject("WScript.Shell");
		      shell.run(url);
		  }
		  var panels = new Array("","panel1","panel2");
			function panel(tab) {
			    for (t=1; t<panels.length; t++) {
			        if (t == tab) {
			            document.getElementById("tab"+t).className = "tabs tabs1";
			            document.getElementById("panel"+t).style.display = "block";
			            document.getElementById('searchText').focus();
						document.getElementById('searchText').select();
			        } else {
			            document.getElementById("tab"+t).className = "tabs tabs0";
			            document.getElementById("panel"+t).style.display = "none";
			        }
			    }
			}
			</script>
		</script>
	    <script language="vbscript">
	        Sub Window_OnLoad
	        window.resizeTo 1000,900 
	        strInstalledDir = GetScriptFolder()
	        username1 = ReadIni(strInstalledDir & "install.ini", "General", "username")
	        password1 = ReadIni(strInstalledDir & "install.ini", "General", "password")
	        username.value = username1
	        password.value = password1
	        chk_LibMessages1 = ReadIni(strInstalledDir & "install.ini", "General", "LibMessages")
	        chk_ShowInstruction1 = ReadIni(strInstalledDir & "install.ini", "General", "Instructions")
	        chk_ClosePlugin1 = ReadIni(strInstalledDir & "install.ini", "General", "ClosePlugin")
	        chk_Download3D1 = ReadIni(strInstalledDir & "install.ini", "General", "Download3D")
	        
	        chk_LibMessages.checked = chk_LibMessages1
	        chk_ShowInstruction.checked = chk_ShowInstruction1
	        chk_ClosePlugin.checked = chk_ClosePlugin1
	        chk_Download3D.checked = chk_Download3D1
	        Call LoginUser(username1,password1)
	        searchText.select
	        dnldsFldr.value = "KiCad Library: " & ReadIni(strInstalledDir & "install.ini", "General", "LibraryFolder") & "\SamacSys_Parts.lib"
	        library.value = ReadIni(strInstalledDir & "install.ini", "General", "LibraryFolder")
	        model_folder.value = ReadIni(strInstalledDir & "install.ini", "General", "3dModelFolder")
	        Randomize()
			RndNo = (CStr(Int(1000000 * Rnd())))
	        'toolbarImg.src = "http:\\componentsearchengine.com\ExtRef\altium\AltiumToolbar.PNG?t=" + RndNo
	        document.getElementById("iframeWelcome").src = "http://componentsearchengine.com/ExtRef/kicad/homepage.htm?t=" + RndNo
	        End Sub
	        Sub checkEnter
  				With document.parentWindow.event
    				if .keycode = 13 then
      					Call MySearch()
    				End if
  				End With
			End sub
	    </script>
